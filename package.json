{
  "name": "htsl-uikit-lexicon",
  "version": "0.1.3",
  "description": "Uikit base for HTSL",
  "scripts": {
    "prebuild": "eslint src test",
    "build": "babel src -d dist",
    "watch": "babel src -d dist -w",
    "pretest": "npm run build",
    "test": "mocha",
    "karma": "karma start",
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.js",
    "prepublish": "npm test",
    "release": "standard-version"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "uikit",
    "HTSL",
    "babelute",
    "DSL"
  ],
  "author": "Gilles Coomans",
  "license": "MIT",
  "dependencies": {
    "clean-embed-video": "git+https://github.com/nomocas/clean-embed-video.git#master",
    "nomocas-webutils": "^0.0.8",
    "perfect-scrollbar": "^0.7.1",
    "mini-wysiwyg": "^0.1.4"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.14.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "standard-version": "^4.0.0"
  }
}
